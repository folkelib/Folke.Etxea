{
  "webroot": "wwwroot",
  "version": "1.0.0-*",
  "packOptions": {
    "licenseUrl": "http://opensource.org/licenses/MIT",
    "projectUrl": "https://github.com/folkelib/Folke.Etxea"
  },

  "dependencies": {
    "Microsoft.AspNetCore.Mvc": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Server.WebListener": "0.1.0-rc2-final",
    "Microsoft.Extensions.Configuration.Json": "1.0.0-rc2-final",
    "Microsoft.Extensions.Configuration.CommandLine": "1.0.0-rc2-final",
    "Microsoft.Extensions.DependencyInjection": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0-rc2-final",
    "Folke.Identity.Elm": "1.1.0-beta-*",
    "Folke.Elm": "1.2.0-beta-*",
    "Folke.Elm.Sqlite": "1.2.0-beta-*",
    "Folke.CsTsService": "1.1.0-beta-*",
    "Folke.Mvc.Extensions": "1.1.0-beta-*",
    "Folke.Identity.Server": "1.3.0-beta-*",
    "Folke.Forum": "1.1.0-beta-*",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0-rc2-final"
  },
  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": {
      "version": "1.0.0-*",
      "imports": "portable-net45+wp80+win8+wpa81+dnxcore50"
    }
  },
  "scripts": {
    "prepublish": [ "npm install", "bower install", "gulp build" ]
  },
  "frameworks": {
    "dnx46": { }
  },
  "commands": {
    "web": "Microsoft.AspNet.Server.Kestrel",
    "csts": "Otsho"
  },
  "buildOptions": {
    "exclude": [
      "wwwroot",
      "node_modules",
      "bower_components"
    ],
    "copyToOutput": {
      "include": [ "res/**/*", "appsettings.*.json", "appsettings.json" ]
    },
    "emitEntryPoint": true
  },
  "publishOptions": {
    "publishExclude": [
      "node_modules",
      "bower_components",
      "**.xproj",
      "**.user",
      "**.vspscc"
    ]
  }
}
